{
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
      "PYTHONPATH": "src/"
  }
}

{
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    { "src": "/api/", "dest": "app.py" },
    { "src": "/api/(.*)", "dest": "app.py" },
    { "src": "/(.*)", "dest": "app.py" }
  ],
  "env": {
      "PYTHONPATH": "."
  }
}