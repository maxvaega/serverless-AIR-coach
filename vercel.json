{
    "redirects": [
        {
          "source": "/",
          "destination": "/api",
          "permanent": true
        }
      ],
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ],
    "env": {
    "PYTHONPATH": "src/"
}
  }