{
    "rewrites": [
        {
          "source": "/api/:path*",
          "destination": "/:path*"
        }
      ],
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ],
    "env": {
    "PYTHONPATH": "src/"
}
  }